@article{cohenkadoshLinkingGABAGlutamate2015,
  title = {Linking {{GABA}} and Glutamate Levels to Cognitive Skill Acquisition during Development},
  author = {Cohen Kadosh, Kathrin and Krause, Beatrix and King, Andrew J. and Near, Jamie and Cohen Kadosh, Roi},
  year = {2015},
  journal = {Human Brain Mapping},
  volume = {36},
  number = {11},
  pages = {4334--4345},
  issn = {1097-0193},
  doi = {10.1002/hbm.22921},
  abstract = {Developmental adjustments in the balance of excitation and inhibition are thought to constrain the plasticity of sensory areas of the cortex. It is unknown however, how changes in excitatory or inhibitory neurochemical expression (glutamate, {$\gamma$}-aminobutyric acid (GABA)) contribute to skill acquisition during development. Here we used single-voxel proton magnetic resonance spectroscopy (1H-MRS) to reveal how differences in cortical glutamate vs. GABA ratios relate to face proficiency and working memory abilities in children and adults. We show that higher glutamate levels in the inferior frontal gyrus correlated positively with face processing proficiency in the children, but not the adults, an effect which was independent of age-dependent differences in underlying cortical gray matter. Moreover, we found that glutamate/GABA levels and gray matter volume are dissociated at the different maturational stages. These findings suggest that increased excitation during development is linked to neuroplasticity and the acquisition of new cognitive skills. They also offer a new, neurochemical approach to investigating the relationship between cognitive performance and brain development across the lifespan. Hum Brain Mapp 36:4334\textendash 4345, 2015. \textcopyright{} 2015 The Authors. Human Brain Mapping Published byWiley Periodicals, Inc.},
  langid = {english},
  keywords = {cognitive development,cortical excitability,face processing,glutamate,magnetic resonance spectroscopy,working memory,γ-aminobutyric acid},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/hbm.22921},
  file = {/home/kevin/Zotero/storage/AD2MWUXP/Cohen Kadosh et al_2015_Linking GABA and glutamate levels to cognitive skill acquisition during.pdf;/home/kevin/Zotero/storage/RE6YTNSJ/hbm.html}
}

  author = {Horvath, Thomas D. and Ihekweazu, Faith D. and Haidacher, Sigmund J. and Ruan, Wenly and Engevik, Kristen A. and Fultz, Robert and Hoch, Kathleen M. and Luna, Ruth Ann and Oezguen, Numan and Spinler, Jennifer K. and Haag, Anthony M. and Versalovic, James and Engevik, Melinda A.},

@article{maruoAdlercreutziaEquolifaciensGen2008,
  title = {Adlercreutzia Equolifaciens Gen. Nov., Sp. Nov., an Equol-Producing Bacterium Isolated from Human Faeces, and Emended Description of the Genus {{Eggerthella}}},
  author = {Maruo, Toshinari and Sakamoto, Mitsuo and Ito, Chiaki and Toda, Toshiya and Benno, Yoshimi},

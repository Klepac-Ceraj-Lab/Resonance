@article{hosomiOralAdministrationBlautia2022,
  title = {Oral Administration of {{Blautia}} Wexlerae Ameliorates Obesity and Type 2 Diabetes via Metabolic Remodeling of the Gut Microbiota},
  author = {Hosomi, Koji and Saito, Mayu and Park, Jonguk and Murakami, Haruka and Shibata, Naoko and Ando, Masahiro and Nagatake, Takahiro and Konishi, Kana and Ohno, Harumi and Tanisawa, Kumpei and Mohsen, Attayeb and Chen, Yi-An and Kawashima, Hitoshi and {Natsume-Kitatani}, Yayoi and Oka, Yoshimasa and Shimizu, Hidenori and Furuta, Mari and Tojima, Yoko and Sawane, Kento and Saika, Azusa and Kondo, Saki and Yonejima, Yasunori and Takeyama, Haruko and Matsutani, Akira and Mizuguchi, Kenji and Miyachi, Motohiko and Kunisawa, Jun},
  year = {2022},
  month = aug,
  journal = {Nature Communications},
  volume = {13},
  number = {1},
  pages = {4477},
  issn = {2041-1723},
  doi = {10.1038/s41467-022-32015-7},
  abstract = {Abstract                            The gut microbiome is an important determinant in various diseases. Here we perform a cross-sectional study of Japanese adults and identify the               Blautia               genus, especially               B. wexlerae               , as a commensal bacterium that is inversely correlated with obesity and type 2 diabetes mellitus. Oral administration of               B. wexlerae               to mice induce metabolic changes and anti-inflammatory effects that decrease both high-fat diet\textendash induced obesity and diabetes. The beneficial effects of               B. wexlerae               are correlated with unique amino-acid metabolism to produce S-adenosylmethionine, acetylcholine, and               l               -ornithine and carbohydrate metabolism resulting in the accumulation of amylopectin and production of succinate, lactate, and acetate, with simultaneous modification of the gut bacterial composition. These findings reveal unique regulatory pathways of host and microbial metabolism that may provide novel strategies in preventive and therapeutic approaches for metabolic disorders.},
  langid = {english},
  note = {[TLDR] A cross-sectional study of Japanese adults identifies the Blautia genus, especially B. wexlerae, as a commensal bacterium that is inversely correlated with obesity and type 2 diabetes mellitus and reveals unique regulatory pathways of host and microbial metabolism that may provide novel strategies in preventive and therapeutic approaches for metabolic disorders.},
  file = {/home/kevin/Zotero/storage/H5HB5A7X/Hosomi et al_2022_Oral administration of Blautia wexlerae ameliorates obesity and type 2 diabetes.pdf}
}


@article{appsAnteriorCingulateGyrus2016,
  title = {The {{Anterior Cingulate Gyrus}} and {{Social Cognition}}: {{Tracking}} the {{Motivation}} of {{Others}}},
  shorttitle = {The {{Anterior Cingulate Gyrus}} and {{Social Cognition}}},
  author = {Apps, Matthew~A.J. and Rushworth, Matthew~F.S. and Chang, Steve~W.C.},
  year = {2016},
  month = may,
  journal = {Neuron},
  volume = {90},
  number = {4},
  pages = {692--707},
  issn = {0896-6273},
  doi = {10.1016/j.neuron.2016.04.018},
  abstract = {The anterior cingulate cortex (ACC) is implicated in a broad range of behaviors and cognitive processes, but it has been unclear what contribution, if any, the ACC makes to social behavior. We argue that anatomical and functional evidence suggests that a specific sub-region of ACC\textemdash in the gyrus (ACCg)\textemdash plays a crucial role in processing social information. We propose that the computational properties of the ACCg support a contribution to social cognition by estimating how motivated other individuals are and dynamically updating those estimates when further evidence suggests they have been erroneous. Notably this model, based on vicarious motivation and error processing, provides a unified account of neurophysiological and neuroimaging evidence that the ACCg is sensitive to costs, benefits, and errors during social interactions. Furthermore, it makes specific, testable predictions about a key mechanism that may underpin variability in socio-cognitive abilities in health and disease.,                                        \textbullet{}               The anterior cingulate gyrus (ACCg) mainly signals other-oriented information                                         \textbullet{}               The ACCg responds to others' motivation and prediction errors                                         \textbullet{}               ACCg variability may lead to variability in social cognition in health and disease                                         \textbullet{}               A computational framework is proposed accounting for ACCg social responses                                 , Apps et~al. review specialized properties of the anterior cingulate gyrus in social cognition and put forward a computational account of how the motivation of other agents is estimated in the anterior cingulate gyrus of human and non-human primates.},
  pmcid = {PMC4885021},
  pmid = {27196973},
  file = {/home/kevin/Zotero/storage/CGA5V3R4/Apps et al_2016_The Anterior Cingulate Gyrus and Social Cognition.pdf}
}

